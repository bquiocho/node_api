<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <br/>
    <% users.forEach(function(item) { %>
        <table style="width:100%; text-align:left;">
          <thead>
            <tr>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Organization</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>
                  <%= item.FName %>
              </td>
              <td>
                  <%= item.LName %>
              </td>
              <td>
                  <%= item.Org %>
              </td>
            </tr>
          </tbody>
        </table>
    <% }); %>

    <br />
        <table style="width:100%;text-align:left;">
          <thead>
            <tr>
              <th>ActionID</th>
              <th>Title</th>
              <th>Due Date</th>
              <th>Due Time</th>
              <th>Details</th>
            </tr>
          </thead>
          <tbody style="text-align:left;">
            <% action_details.forEach(function(item) { %>
            <tr>
              <td>
                  <%= item.ActionID %>
              </td>
              <td>
                  <%= item.Title %>
              </td>
              <td>
                  <%= item.DueDate %>
              </td>
              <td>
                  <%= item.DueTime %>
              </td>
              <td>
                  <%= item.Details %>
              </td>
            </tr>
                <% }); %>
          </tbody>
        </table>


  </body>
</html>
